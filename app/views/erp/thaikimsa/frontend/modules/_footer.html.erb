    <div class="footer-widgets">
        <div class="footer-widgets__inner">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6 col-md-4">
                        <div class="footer-col-inner">
                            <!-- Footer Logo -->
                            <div class="footer-logo footer-logo--has-txt">
                                <a href="index.html">
                                    <img src="/thaikimsa/assets/images/logo-footer.png" srcset="/thaikimsa/assets/images/logo-footer@2x.png" alt="" class="footer-logo__img">
                                    <div class="footer-logo__heading">
                                        <h5 class="footer-logo__txt">Thái Kim Sa</h5>
                                        <span class="footer-logo__tagline">
                                            Tử Vi Và Phong Thủy Cổ Điển
                                        </span>
                                    </div>
                                </a>
                            </div>
                            <!-- Footer Logo / End -->
