                                <h4 class="widget__title">Dịch Vụ Xem Tử Vi</h4>
                                <div class="widget__content">
                                    <ul class="posts posts--simple-list posts--simple-list-numbered">
                                        <% (0..2).each do |blog| %>
                                            <li class="posts__item posts__item--category-1">
                                                <div class="posts__inner">
                                                    <div class="posts__cat">
                                                        <span class="label posts__cat-label">Tử Vi</span>
                                                    </div>
                                                    <h6 class="posts__title">
                                                        <%= link_to 'Dự đoán ngày mới 06-06-2018 cho 12 con giáp', '', title: '' %>                                                            
                                                    </h6>
                                                    <time datetime="2018-09-22" class="posts__date">06-06-2018</time>
                                                </div>
                                            </li>
                                        <% end %>
                                    </ul>
                                </div>
                            </div>
                            <!-- Widget: Popular Posts / End -->
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-4">
                        <div class="footer-col-inner">
                            <!-- Widget: Popular Posts / End -->
                            <div class="widget widget--footer widget-popular-posts">
                                <h4 class="widget__title">Dịch Vụ Xem Phong Thủy</h4>
                                <div class="widget__content">
                                    <ul class="posts posts--simple-list posts--simple-list-numbered">
