                            <div class="widget widget--footer widget-contact-info">
                                <div class="widget__content">
                                    <div class="widget-contact-info__body info-block">
                                        <div class="info-block__item info-block__item-footer">
                                            <h6 class="info-block__heading">Địa Chỉ</h6>
                                            <a class="info-block__link" href="">Chung cư Minh Thành 259 Lê Văn Lương, Tân Quy, Quận 7, Hồ Chí Minh</a>
                                        </div>
                                        <div class="info-block__item info-block__item-footer">
                                            <h6 class="info-block__heading">Số Điện Thoại</h6>
                                            <a class="info-block__link" href="tel:info@alchemists.com">0933 808 368</a>
                                        </div>
                                        <div class="info-block__item info-block__item-footer">
                                            <h6 class="info-block__heading">Địa Chỉ Email</h6>
                                            <a class="info-block__link" href="mailto:thaikimsa@gmail.com">thaikimsa@gmail.com</a>
                                        </div>
                                        <div class="info-block__item info-block__item--nopadding">
                                            <ul class="social-links">
                                                <li class="social-links__item facebook">
                                                    <a href="" class="social-links__link">
                                                        <i class="fa fa-facebook"></i>
                                                        Facebook
                                                    </a>
                                                </li>
                                                <li class="social-links__item twitter">
                                                    <a href="" class="social-links__link">
                                                        <i class="fa fa-twitter"></i>
                                                        Twitter
                                                    </a>
                                                </li>
                                                <li class="social-links__item google-plus">
                                                    <a href="" class="social-links__link">
                                                        <i class="fa fa-google-plus"></i>
                                                        Google+
                                                    </a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
