                                        <% end %>
                                    </ul>
                                </div>
                            </div>
                            <!-- Widget: Popular Posts / End -->
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-12">
                        <div class="footer-col-inner">
                            <!-- Widget: Tagcloud -->
                            <div class="widget widget--footer widget-tagcloud">
                                <h4 class="widget__title">Tags Nổi Bật</h4>
                                <div class="widget__content">
                                    <div class="tagcloud">
                                        <% (0..3).each do |tag| %>
                                            <a href="" class="btn btn-default btn-xs btn-outline btn-sm">xem tử vi</a>
                                            <a href="" class="btn btn-default btn-xs btn-outline btn-sm">xem phong thủy</a>
                                            <a href="" class="btn btn-default btn-xs btn-outline btn-sm">kích hoạt tài vận</a>
