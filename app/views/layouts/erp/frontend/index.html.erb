                <div class="container">
                    <div class="header__primary-inner">
                        <!-- Main Navigation -->
                        <nav class="main-nav clearfix">
                            <ul class="main-nav__list">
                                <li class="active"><%= link_to 'Trang Chủ', erp_thaikimsa.root_path, title: '' %></li>
                                <li class="">
